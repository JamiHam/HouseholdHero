                  <Image url="@../../../householdhero-logo-white-fridge.png" />
               </image>
            </ImageView>
            <VBox layoutX="2.0" layoutY="201.0" spacing="5">
               <children>
                  <Button id="button-fridge" mnemonicParsing="false" prefHeight="40.0" prefWidth="210.0" text="Fridge" textAlignment="CENTER">
