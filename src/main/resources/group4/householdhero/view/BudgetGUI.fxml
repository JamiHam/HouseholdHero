                        <Insets left="3.0" right="3.0" />
                     </GridPane.margin>
                  </DatePicker>
                  <Label text="End Date" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                  <DatePicker fx:id="endDatePicker" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
