<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="322.0" prefWidth="546.0" spacing="20.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group4.householdhero.view.FridgeController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="AddButton" onAction="#addProduct" text="Switch to Secondary View" />
      <TableView fx:id="fridgeTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="categoryColumn" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="priceColumn" prefWidth="94.0" text="Price" />
            <TableColumn fx:id="bestBeforeColumn" prefWidth="106.0" text="Best Before" />
            <TableColumn fx:id="editColumn" prefWidth="75.0" text="Edit" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
